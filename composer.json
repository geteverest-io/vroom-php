{
  "name": "webstack/vroom-php",
  "license": "MIT",
  "description": "Vroom PHP",
  "keywords": ["VROOM"],
  "homepage": "https://webstack.nl",
  "authors": [
    {
      "name": "Marco Wansinck",
      "email": "marco@webstack.nl"
    }
  ],
  "require": {
    "php": "^8.0|^7.3",
    "ext-json": "*",
    "symfony/http-client": "^4.4|^5.1",
    "symfony/property-info": "^4.4|^5.1",
    "symfony/serializer": "^4.4|^5.1"
  },
  "require-dev" : {
    "phpunit/phpunit": "^6.5.5 || ^7.0 || ^8.0 || ^9.3"
  },
  "autoload": {
    "psr-4": {
      "Webstack\\Vroom\\": "src/",
      "Webstack\\Vroom\\Tests\\": "tests/"
    }
  },
  "config" : {
    "sort-packages": true,
    "lock": false
  }
}
